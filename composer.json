{
    "name": "dmitrybubyakin/nova-medialibrary-field",
    "description": "Laravel Nova field for managing the Spatie media library.",
    "keywords": [
        "laravel",
        "nova",
        "medialibrary"
    ],
    "homepage": "https://github.com/dmitrybubyakin/nova-medialibrary-field",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Bubyakin",
            "email": "dimabubyakin@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "spatie/laravel-medialibrary": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "DmitryBubyakin\\NovaMedialibraryField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DmitryBubyakin\\NovaMedialibraryField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
